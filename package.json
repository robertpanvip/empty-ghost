{
  "name": "empty-ghost",
  "description": "该组件允许开发者在不影响页面布局的情况下插入内容，同时通过 React 的 ref 功能获取到其子元素的真实 DOM 并且事件传递",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build-site": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "compile": "es-pkg compile",
    "unpkg": "es-pkg unpkg",
    "doc": "es-pkg doc",
    "publish": "es-pkg publish"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "react-dom": "^18.2.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "es-pkg": "latest",
    "vite-plugin-checker": "^0.6.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "empty-ghost"
  ],
  "homepage": "https://github.com/robertpanvip/empty-ghost.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/robertpanvip/empty-ghost.git"
  },
  "bugs": {
    "url": "https://github.com/robertpanvip/empty-ghost/issues"
  },
  "author": "robertpanvip",
  "license": "ISC"
}